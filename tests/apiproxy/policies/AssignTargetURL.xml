<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignTargetURL">
    <DisplayName>AssignTargetURL</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>target.url</Name>
        <Ref>httpbin_url</Ref>
        <Value>http://httpbin.org/{request.verb}</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>